    "gulp-zip": "^3.0.2",
    "jshint-stylish": "^2.0.1",
    "mocha": "3.4.2",
    "sinon": "2.3.8",
    "tsify": "^3.0.4",
    "typescript": "^2.6.2",
    "uglifyify": "^3.0.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.44",
    "@types/sinon": "^4.1.2"
  },
